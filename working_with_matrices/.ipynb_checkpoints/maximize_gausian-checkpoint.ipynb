   "source": [
    "# Change the last argument in f() to maximize the output.\n",
    "\n",
    "from math import *\n",
    "\n",
    "def gaussian(mu, sigma2, x):\n",
    "    g = 1/sqrt(2*pi*sigma2) * exp(- 0.5 * (x - mu)**2/sigma2)\n",
    "    \n",
    "    return g\n",
    "\n",
    "first_gausian = gaussian(10, 4, 8)"
